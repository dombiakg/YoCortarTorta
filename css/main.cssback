.button-list1 {
    background-color : black;
    animation-name: entrada-superior;
    animation-duration: 3s; /*duracion*/
    animation-iteration-count: 1; /*cantidad de veces que repite*/
    animation-timing-function: ease-in-out;
    animation-delay: 0,3s; /*tarda en empezar*/
}
.button-list2 {
    animation-name: entrada-superior;
    animation-duration: 3s; /*duracion*/
    animation-iteration-count: 1; /*cantidad de veces que repite*/
    animation-timing-function: ease-in-out;
    animation-delay: 1s; /*tarda en empezar*/
}
